{
  "name": "sannatour",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "schema:drop": "ts-node ./node_modules/typeorm/cli.js schema:drop",
    "schema:sync": "ts-node ./node_modules/typeorm/cli.js schema:sync",
    "seed:config": "ts-node ./node_modules/typeorm-seeding/dist/cli.js config",
    "seed:run": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed",
    "seed": "npm run schema:drop && npm run schema:sync && NODE_ENV=development npm run seed:run",
    "seed:staging": "npm run schema:drop && npm run schema:sync && NODE_ENV=staging npm run seed:run",
    "seed:prod": "npm run schema:drop && npm run schema:sync && NODE_ENV=product npm run seed:run",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "NODE_ENV= nest start",
    "start:staging": "NODE_ENV=staging nest start",
    "start:prod": "NODE_ENV=production nest start",
    "dev": "NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix --max-warnings 0",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "7.0.7",
    "@nestjs/core": "7.0.7",
    "@nestjs/jwt": "^7.0.0",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-express": "7.0.7",
    "@nestjs/swagger": "4.5.1",
    "@nestjs/typeorm": "^7.0.0",
    "@nestjsx/crud": "^4.4.2",
    "@nestjsx/crud-typeorm": "^4.4.2",
    "@types/lodash": "^4.14.157",
    "aws-sdk": "^2.713.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "0.2.3",
    "class-validator": "0.11.1",
    "config": "^3.3.1",
    "csv-parser": "^2.3.3",
    "dotenv": "^8.2.0",
    "fs": "^0.0.1-security",
    "husky": "^4.2.5",
    "lodash": "^4.17.15",
    "moment": "2.24.0",
    "nan": "^2.14.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.0.2",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "6.5.5",
    "slug": "^3.1.0",
    "swagger-ui-express": "4.1.4",
    "typeorm": "^0.2.25"
  },
  "devDependencies": {
    "@nestjs/cli": "7.1.2",
    "@nestjs/schematics": "7.0.0",
    "@nestjs/testing": "7.0.7",
    "@types/express": "4.17.6",
    "@types/faker": "^4.1.11",
    "@types/jest": "^26.0.4",
    "@types/node": "10.17.3",
    "@types/supertest": "2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "2.28.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb-typescript": "^7.2.1",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-only-warn": "^1.0.2",
    "jest": "^26.1.0",
    "prettier": "2.0.4",
    "supertest": "4.0.2",
    "ts-jest": "^26.1.1",
    "ts-loader": "7.0.0",
    "ts-node": "8.8.2",
    "tsconfig-paths": "3.9.0",
    "typeorm-seeding": "^1.6.1",
    "typescript": "^3.9.6"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
